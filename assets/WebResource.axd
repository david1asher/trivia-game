
.ClalitGridWrapper { direction: rtl; }

.ClalitGridTable { padding-bottom: 5px; border-collapse: collapse; 
    border-bottom: 1px solid #cdcdcd; width: 100%; }

.ClalitGridTable caption { display: none; }

.ClalitGridTable thead tr { height: 33px; }
.ClalitGridTable tr th { font-family: Arial; 
    background: url(WebResource.axd?d=-sEBTCOa5Wm7Nn7dp8JxDkXbggUFBSz3l5MnW8wBkPoQ4m51haQUdy_XuetZ7MTO4cDOCD8W4fnalWCCBqukGFmWOtJt9crZu-5x8L4g91yjHWeWQDbP7hr8R4Fli84nghmb6R5pp7h80yXP70JkRw6eDRHAFp-rWoabhjx23nc1&t=637658363300000000) repeat-x; 
    color: #000; font-weight: normal; padding-right: 7px; height: 33px; text-align: right; vertical-align: middle; }
.ClalitGridTable th a { color: #000; text-decoration: none; }
.ClalitGridTable th a:hover { cursor: pointer; }

.ClalitGridTable tbody tr { white-space: normal; text-align: right; padding: 0 8px 5px 8px; 
    vertical-align: middle; min-height: 54px; border-bottom: 1px solid #cfd3d5; }

.ClalitGridTable tr td { padding: 5px 8px 5px 8px; vertical-align: middle; }

