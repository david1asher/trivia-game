.ClalitGridRow { background-color: White; vertical-align: middle; }

.ClalitGridLinkButtonItem { margin-right: 5px; text-decoration: underline; }
.ClalitGridOpenDescriptionItem { margin-right: 5px; text-decoration: underline; }

.ClalitGridCounterTitle { font-family: Arial; padding-top: 0; margin-top: 0; margin-bottom: 6px; 
    vertical-align: top; text-align: right; }

.ClalitGridSelected { cursor: default; }
.ClalitGridSelected a { /*cursor: default;*/ }

.ClalitGridUpperCloseLink { float: left; color: #00aeed; text-decoration: none; cursor: pointer; }
.ClalitGridUpperCloseLink:hover { display: inline-block; text-decoration: underline; cursor: pointer; }
.ClalitGridCloseImg { background-image: url(WebResource.axd?d=dwhfBYJXWT_dhRhAQEfF3pfztEp3Iu8paApg5erO9Q-NVD9UWgps4SmYvvg-BqPVhbkiOKuemoaOtRl9o9KNpqy-ZajLSjIZAOIFXpjUssKlIZLTSmHOgy3_7D3yF1TUX_pupS8UT5-gBDxLKtti5D_t7wPdlcSXUpx3Iy2pLq41&t=637658363300000000); 
    width: 21px; height: 17px; background-repeat: no-repeat; background-position: center; 
    cursor: pointer; display: block; float: left; padding-bottom: 7px; }


.ClalitGridPagerContainer { text-align: center; direction: rtl; vertical-align: middle; }
.ClalitGridPagerContainer * { vertical-align: middle; display: inline-block; }
.ClalitGridPagerContainer a { 
    margin-right: 5px; text-decoration: none; color: #005ea6; font-weight: 700; }
.ClalitGridPagerContainer .active-page { 
    padding: 3px 7px 3px 7px; color: #fff; font-weight: 700; background: #005ea6; border-radius: 5px; }
.ClalitGridPagerContainer a:hover { text-decoration: underline; cursor: pointer; }
.ClalitGridPagerContainer span { margin-right: 5px; color: Black; }
.ClalitGridPagerContainer img { margin-right: 5px; margin-bottom: 1px; }
.OnlineCustomGridPagerLeftArrow, .OnlineCustomGridPagerRightArrow { 
    color: #005ea6 !important; font-weight: bold; }

.ClalitGridDescriptionDiv { }

.NoDisplay { display: none; }
.DescBlockCell {width: 100%;}
.DescBlockRow {width: 100%; }
.OnlineCustomGridPagerItemList
{
    padding: 0;
    margin: 0;
    list-style-type: none;
}
.OnlineCustomGridPagerItemList li
{
    padding: 0;
    margin: 0;
}
.ClalitCustomGridActionList {list-style-type: none; margin:0; padding:0;}
